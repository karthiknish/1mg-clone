{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "productname": {
      "type": "string"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "manufacturer": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "productname"
    },
    "ratings": {
      "collection": "ratings",
      "via": "product"
    },
    "price": {
      "type": "decimal"
    },
    "category": {
      "model": "categories",
      "via": "products"
    }
  }
}
